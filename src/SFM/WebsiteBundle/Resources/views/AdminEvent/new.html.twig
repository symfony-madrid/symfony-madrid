{% extends 'SFMWebsiteBundle::adminlayout.html.twig' %}
{%block content %}
<h1>Event creation</h1>

<form action="{{ path('event_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('event') }}">
            Back to the list
        </a>
    </li>
</ul>
{{ tinymce_init() }}
{% endblock %}
